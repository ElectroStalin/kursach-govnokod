docktype
html
    head
        meta(charset='utf8')
        title Main
        meta(
        name='viewport'
        content='width=devise-width, install-scale=1.0')
        style
            include ../css/style.css
    body
        div(class='wrapper container')
            header
                img( src='../Images/head.png' alt='logo')
                form( name='search' action='#' method='get' class='form-inline form-search pull-right')
                    div( class='input-group')
                        label( class='sr-only' for='searchInput')
                        input( class='form-control' id='searchInput' type='text' name='search' placeholder='поиск')
                        div( class='input-group-btn')
                            button( type='submit' class='btn btn-primary') ОК
            nav( class='nav navbar-default')
                ul( class='nav navbar-nav')
                    li( class='active')
                        a( href='/') Главная
                    li
                        a( href='/AboutUs') О Нас
                    li
                        a( href='/new') Добавить тест

            div(class='heading')
                h1 Добавить тест
            div(class='row')
                aside(class='col-md-7')
                    ul( class='list-group submenu')
                        li(class='list-group-item ')
                            a( href='/Theory') О теории тестов
                        li(class='list-group-item ')
                            a( href='/ClassicTheory/') Классическая теория
                        li(class='list-group-item ')
                            a( href='/ModernTheory/') Современная теория
                        li(class='list-group-item active')
                            a( href='/Feedback/') Оставить отзыв

                aside(class='col-md-17')
                    form( name='feedback' action='#' method='get' class='form-horizontal pull-left')
                        div( class='input-group')
                            label( class='sr-only' for='inputName')
                            input(class='form-control' id='inputName' name='name' placeholder='имя' pattern='^[a-zA-Z-А-Яа-яЁё\s]+$')
                        div( class='input-group')
                            label( class='sr-only' for='inputName')
                            textarea( class='form-control' name='comment' cols='100' rows='6')
                        div(class='sbm')
                            button( type="submit" class="btn btn-primary") ОТПРАВИТЬ



